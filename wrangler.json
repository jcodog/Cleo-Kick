{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "cleo-kick",
  "main": "src/index.ts",
  "compatibility_date": "2024-10-01",
  "dev": {
    "inspector_port": 9229,
    "port": 3000
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "d1_databases": [
    {
      "binding": "ERROR_LOG_DB",
      "database_name": "cleo-kick",
      "database_id": "4c98a884-89e3-4813-9d14-64b36dda2bca"
    }
  ],
  "preview_urls": false,
  "workers_dev": false,
  "routes": [
    {
      "custom_domain": true,
      "pattern": "kickbot.cleoai.cloud"
    }
  ]
}
