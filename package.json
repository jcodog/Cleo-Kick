    "kick-api-types": "^1.0.12"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251106.1",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "prisma": "^6.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7",
    "wrangler": "^4.46.0"
  }
